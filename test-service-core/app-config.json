{
  "Guytp.WebApi": {
    "Api": {
      "Version": 1,
      "IsCorsAllowingAll": true,
      "IsEnumSerialisedAsString": true,
      "BindingUri": "http://+:50000",
    },
    "JwtAuth": {
      "IsMiddlewareEnabled": true,
      "Issuer": "urn:com.lts-api.sso",
      "Audience": "urn:com.lts-api",
      "CertificateData":  ""
    },
    "Swagger": {
      "IsEnabled": true,
      "OverrideRootUrl": null,
      "IsUiEnabled": true,
      "SwaggerRoute": "_md/docs",
      "SwaggerUiRoute": "_md"
    },
    "ExceptionHandling": {
      "IsMiddlewareEnabled": true,
      "IsStackTraceIncluded": true,
      "IsLoggingEnabled": true,
      "LogLevel": "Error"
    }
  }
}